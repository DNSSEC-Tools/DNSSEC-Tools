diff --git a/security/nss/lib/ssl/Makefile b/security/nss/lib/ssl/Makefile
index c29d3d5..a8204b6 100644
--- a/security/nss/lib/ssl/Makefile
+++ b/security/nss/lib/ssl/Makefile
@@ -42,6 +42,7 @@ endif
 # MOZ_DANE
 ifdef MOZ_DNSSEC
 DEFINES += $(VAL_CFLAGS)
