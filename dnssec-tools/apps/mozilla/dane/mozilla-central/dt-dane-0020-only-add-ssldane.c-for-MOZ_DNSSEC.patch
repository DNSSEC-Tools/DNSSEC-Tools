 endif
 
 #######################################################################
diff --git a/security/nss/lib/ssl/manifest.mn b/security/nss/lib/ssl/manifest.mn
index ac38b5a..3bb28a2 100644
--- a/security/nss/lib/ssl/manifest.mn
+++ b/security/nss/lib/ssl/manifest.mn
@@ -44,7 +44,6 @@ CSRCS = \
 	cmpcert.c \
 	sslinfo.c \
 	ssl3ecc.c \
-	ssldane.c \
 	$(NULL)
 
 LIBRARY_NAME = ssl
